    "version": "0.2.0",
    "configurations": [
        {
            "logging": {"moduleLoad": true},
            "type": "cppvsdbg",
            "request": "launch",
            "name": "debug-client",
            "program": "${workspaceFolder}/build/debug/client.exe",
            "console": "integratedTerminal",
            "args": [],
            "cwd": "${workspaceFolder}/build/debug",
            "visualizerFile": "${workspaceFolder}/lib/EASTL/doc/EASTL.natvis",
            "requireExactSource": true,
        },
        {
            "type": "cppvsdbg",
            "request": "launch",
            "name": "debug_server",
            "program": "${workspaceFolder}/build/debug/server.exe",
            "console": "integratedTerminal",
            "args": [],
            "cwd": "${workspaceFolder}/build/debug",
            "visualizerFile": "${workspaceFolder}/lib/EASTL/doc/EASTL.natvis"
        },
        {
            "type": "cppvsdbg",
            "request": "launch",
            "name": "release-client",
            "program": "${workspaceFolder}/build/release/client.exe",
            "console": "integratedTerminal",
            "args": [],
            "cwd": "${workspaceFolder}/build/release",
            "visualizerFile": "${workspaceFolder}/lib/EASTL/doc/EASTL.natvis"
        },
    ]
}